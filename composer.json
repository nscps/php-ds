{
    "type": "library",
    "license": "MIT",
    "name": "nscps/php-ds",
    "require": {
        "php": ">=7.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "fzaninotto/faker": "^1.9",
        "symfony/var-dumper": "5.1.*"
    },
    "autoload": {
        "psr-4": {
            "Nscps\\Ds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nscps\\Ds\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "tests": [
            "vendor/bin/phpunit"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html=coverage"
        ]
    }
}
